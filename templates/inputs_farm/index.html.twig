{% extends 'base.html.twig' %}

{% block title %}InputsFarm index{% endblock %}

{% block body %}
    <h1>InputsFarm index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>ChickNumber</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for inputs_farm in inputs_farms %}
            <tr>
                <td>{{ inputs_farm.id }}</td>
                <td>{{ inputs_farm.chickNumber }}</td>
                <td>
                    <a href="{{ path('inputs_farm_show', {'id': inputs_farm.id}) }}">show</a>
                    <a href="{{ path('inputs_farm_edit', {'id': inputs_farm.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('inputs_farm_new') }}">Create new</a>
{% endblock %}
