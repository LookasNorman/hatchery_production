{% extends 'base.html.twig' %}

{% block title %}InputsFarm{% endblock %}

{% block body %}
    <h1>InputsFarm</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ inputs_farm.id }}</td>
        </tr>
        <tr>
            <th>ChickNumber</th>
            <td>{{ inputs_farm.chickNumber }}</td>
        </tr>
        </tbody>
    </table>

    <a href="{{ path('inputs_farm_index') }}">back to list</a>
    {% if is_granted('ROLE_MANAGER') %}
        <a href="{{ path('inputs_farm_edit', {'id': inputs_farm.id}) }}">edit</a>
    {% endif %}
    {% if is_granted('ROLE_ADMIN') %}
        {{ include('inputs_farm/_delete_form.html.twig') }}
    {% endif %}
{% endblock %}
