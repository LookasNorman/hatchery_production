<form method="post" action="{{ path('eggs_inputs_details_delete', {'id': eggs_inputs_detail.id}) }}"
      onsubmit="return confirm('{% trans %}eggs_inputs_details.delete.confirm{% endtrans %}');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ eggs_inputs_detail.id) }}">
    <button class="btn btn-danger">
        {% trans %}eggs_inputs_details.delete.button.delete{% endtrans %}
    </button>
</form>
